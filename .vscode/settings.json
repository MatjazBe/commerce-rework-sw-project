{
  "window.title": "commerce-rework-sw-project${separator}${dirty}${activeEditorMedium}${separator}${appName}",
  "git.ignoredRepositories": [
    ".devcontainer"
  ],
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "/bin/bash",
      "icon": "terminal-bash"
    }
  },
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.cwd": "${workspaceFolder}/app/",
  "terminal.integrated.allowChords": false,
  "sqltools.connections": [
    {
      "mysqlOptions": {
        "authProtocol": "default"
      },
      "previewLimit": 50,
      "server": "db_dev",
      "port": 3306,
      "driver": "MariaDB",
      "name": "db",
      "database": "drupal_dev",
      "username": "root",
      "password": "drupal_dev"
    }
  ],
  "git.autofetch": true,
  "[php]": {
    "editor.defaultFormatter": "wongjn.php-sniffer"
  },
  "phpcs.executablePath": "/project/app/vendor/bin/phpcs",
  "phpSniffer.standard": "/project/app/vendor/drupal/coder/coder_sniffer/DrupalPractice,/project/app/vendor/drupal/coder/coder_sniffer/Drupal"
}
